<div class="total-rows-info">
  <b>Total</b>: <span id="total-set-rows">{{ total }}</span>
</div>
<div id="save-set-results">
  <a href="#">(Save Selected)</a>
</div>
<table id="set-elements">
  <thead>
    <tr>
      <td>
        <input type="checkbox" id="select-all-set-elements" title="Click to select or deselect all elements" />
      </td>
      <th>
        Elements
      </th>
    </tr>
  </thead>
  <tbody>
    {{#elements}}
    <tr>
      <td>
        <input type="checkbox" class="set-element-selection" title="Click to select element" />
      </td>
      <td>
        <a class="view-document-link" href="#{{ id }}">{{ context }}</a>
      </td>
    </tr>
    {{/elements}}
  </tbody>
</table>
