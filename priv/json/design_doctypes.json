{
   "_id": "_design/doctypes",
   "views": {
       "all": {
           "map": "function(doc) { if (doc.category == 'doctype')  { emit(doc._id, doc) }}"
       },
       "all_simple": {
           "map": "function(doc) { if (doc.category == 'doctype')  { emit(doc._id, doc.description) }}"
       }
   }
}
