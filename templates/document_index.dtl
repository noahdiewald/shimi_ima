<table>
  {% for row in rows %}
  <tr>
    <th>
      <a href="#{{ row.id }}" class="view-document-link">
        <ul class="head-elements">
        {% for elem in row.key %}<li>{{ elem }}</li>{% endfor %}
        </ul>
      </a>
    </th>
    <td>
      <ul class="reversal-elements">
      {% for elem in row.value %}
        {% if elem %}<li>{{ elem }}</li>{% endif %}
      {% endfor %}
      </ul>
    </td>
  </tr>
  {% endfor %}
</table>
