<h2 class="ui-widget ui-widget-header ui-corner-tl">Document Index</h2>
  
<table>
  {% for row in rows %}
  <tr>
    <th>
      <a href="#{{ row.id }}" class="view-document-link">
        <ul class="head-elements">
        {% for elem in row.key %}<li>{{ elem }}</li>{% endfor %}
        </ul>
      </a>
    </th>
    <td>
      <ul>
      {% for elem in row.value %}<li>{{ elem }}</li>{% endfor %}
      </ul>
    </td>
  </tr>
  {% endfor %}
</table>
