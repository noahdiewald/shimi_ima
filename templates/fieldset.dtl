<div 
  data-fieldset-id="{{ _id }}"
  data-doctype-id="{{ doctype_info._id }}"
  data-project-id="project-{{ project_info._id }}"
  class="fields 
    {% if multiple %}
    ui-widget ui-widget-content ui-corner-all padded
    {% endif %}" 
>

  {% if multiple %}

  <button class="remove-button">Remove</button>

  {% endif %}
  
</div>
