{
  "_id": "_design/charseqs",
  "version": 1,
  "views": {
    "all": {
      "map": "function(doc) { 
        if (doc.category == 'charseq')  { emit(doc._id, doc) }
      }"
    },
    "all_simple": {
      "map": "function(doc) { 
        if (doc.category == 'charseq')  { emit(doc._id, doc.description) }
      }"
    }
  },
  "validate_doc_update": "{% include 'javascript_charseq_validation.dtl' %}"
}
