{% extends "base.dtl" %}

{% block page_title %}Configuration: {{ name }}{% endblock %}

{% block header %}
<h1>Configuration: {{ name }}</h1>
<em>{{ description }}</em>
{% endblock %}

{% block javascript %}
    <script type="text/javascript" src="/js/config.js"></script>
{% endblock %}

{% block dialogs %}
{% include 'config_doctype_dialog.dtl' %}
{% include 'config_fieldset_dialog.dtl' %}
{% include 'config_field_dialog.dtl' %}
