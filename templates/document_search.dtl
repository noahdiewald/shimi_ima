{% if rows %}
<div class="total-rows-info">
  <b>Total</b>: {{ rows|length }}
</div>
<table class="search-results">
  {% for row in rows %}
  <tr>
    <th>
      <a 
         href="#{{ row.id }}" 
         class="view-document-link">
        {% if index_listing %}
        {% for elem in row.key %}
        {{ elem|last|escape }}{% if not forloop.last %},{% endif %}
        {% endfor %}
        {% else %}
        {{ row.key|last|escape }}
        {% endif %}
      </a>
    </th>
    <td {% if row.id|length == row.value|length %}
        class="search-result-field-id search-result-context"
        data-field-field="{{ row.value }}">
      <a href="#">{{ row.value }}</a>
        {% else %}
        class="search-result-context">
      <a href="#{{ row.id }}" class="view-document-link">
        {{ row.value|force_escape }}
      </a>
        {% endif %}
    </td>
  </tr>
  {% endfor %}
</table>
{% else %}
<em>No Results</em>
{% endif %}
