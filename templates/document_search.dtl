{% if rows %}
<div class="total-rows-info">
  <b>Total</b>: {{ rows|length }}
</div>
<table>
  {% for row in rows %}
  <tr>
    <th>
      <a 
         href="#{{ row.id }}" 
         class="view-document-link">
        {{ row.key|last|force_escape }}
      </a>
    </th>
  </tr>
  {% endfor %}
</table>
{% else %}
<em>No Results</em>
{% endif %}
